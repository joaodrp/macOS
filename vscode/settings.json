{
    "git.autofetch": false,
    "security.promptForLocalFileProtocolHandling": false,
    "security.workspace.trust.untrustedFiles": "open",
    "claudeCode.preferredLocation": "panel",
    "workbench.secondarySideBar.defaultVisibility": "hidden",
    "python.defaultInterpreterPath": "/opt/homebrew/bin/python3",
    "claudeCode.selectedModel": "default",
    "claudeCode.initialPermissionMode": "plan",
    "claudeCode.useTerminal": true,
    "editor.fontFamily": "JetBrainsMono Nerd Font,Mononoki,Iosevka",
    "editor.fontSize": 12.5,
    "makefile.configureOnOpen": false,
    "workbench.colorTheme": "Gruvbox Dark Hard",
    "window.autoDetectColorScheme": true,
    "workbench.preferredDarkColorTheme": "Gruvbox Dark Hard",
    "workbench.preferredLightColorTheme": "Gruvbox Light Hard",
    "terminal.integrated.defaultProfile.osx": "fish",
    "workbench.settings.showAISearchToggle": false,
    "chat.disableAIFeatures": true,
    "editor.inlineSuggest.enabled": false,
    "github.copilot.enable": {
        "*": false
    },
    "github.copilot.inlineSuggest.enable": false,
    "github.copilot.nextEditSuggestions.enabled": false,
    "git.generateCommitMessageWorkingChangesOnly": false,
    "scm.showCommitMessageAIPrompts": false,
    "[markdown]": {
        "editor.defaultFormatter": "yzhang.markdown-all-in-one"
    },
    "workbench.startupEditor": "none"
}
